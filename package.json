{
  "name": "snabbdom-pragma",
  "version": "2.2.0",
  "description": "'NotReact.createElement' pragma although for snabbdom",
  "keywords": [
    "snabbdom",
    "pragma",
    "jsx",
    "createElement",
    "Bubl√©",
    "Buble",
    "Babel",
    "Traceur"
  ],
  "author": "Swizz <gerodel.quentin@gmail.com>",
  "license": "MIT",
  "repository": "git@github.com:Swizz/snabbdom-pragma.git",
  "main": "dist/snabbdom-pragma.common.js",
  "module": "src/snabbdom-pragma.js",
  "jsnext:main": "src/snabbdom-pragma.js",
  "types": "snabbdom-pragma.d.ts",
  "devDependencies": {
    "ava": "^0.19.1",
    "babel-core": "^6.24.0",
    "babel-plugin-transform-react-jsx": "^6.8.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-stage-2": "^6.24.1",
    "bili": "^0.15.3",
    "buble": "^0.15.2",
    "eslint-config-xo-swizz": "^0.11.0",
    "snabbdom": "^0.6.7",
    "traceur": "^0.0.111",
    "typescript": "^2.3.2",
    "xo": "^0.18.2"
  },
  "dependencies": {
    "extend": "^3.0.0"
  },
  "scripts": {
    "lint": "./node_modules/.bin/xo",
    "test:prebuild": "./node_modules/.bin/ava --match 'trans -*' --match 'src -*'",
    "test:postbuild": "./node_modules/.bin/ava --match 'dist -*'",
    "build": "./node_modules/.bin/bili"
  },
  "ava": {
    "files": [
      "test/*-test.js"
    ],
    "require": [
      "babel-register"
    ]
  },
  "xo": {
    "semicolon": false,
    "space": true,
    "extends": "xo-swizz",
    "rules": {
      "import/no-dynamic-require": "off",
      "capitalized-comments": "off"
    },
    "ignores": [
      "test/**/transform-*.js"
    ]
  },
  "bili": {
    "entry": "src/snabbdom-pragma.js",
    "js": "buble",
    "resolve": true,
    "buble": {
      "objectAssign": "fn.assign",
      "namedFunctionExpressions": true
    }
  },
  "babel": {
    "presets": [
      "es2015",
      "stage-2"
    ]
  }
}
