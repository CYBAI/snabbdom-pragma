{
  "name": "snabbdom-pragma",
  "version": "1.5.0",
  "description": "'NotReact.createElement' pragma although for snabbdom",
  "keywords": [
    "snabbdom",
    "pragma",
    "jsx",
    "createElement",
    "Bubl√©",
    "Buble",
    "Babel"
  ],
  "author": "Swizz <gerodel.quentin@gmail.com>",
  "license": "MIT",
  "repository": "git@github.com:Swizz/snabbdom-pragma.git",
  "main": "dist/index.js",
  "devDependencies": {
    "ava": "^0.17.0",
    "babel-plugin-transform-react-jsx": "^6.8.0",
    "buble": "^0.15.2",
    "rollup": "^0.41.3",
    "traceur": "^0.0.111"
  },
  "dependencies": {
    "extend": "^3.0.0",
    "snabbdom": "^0.6.1"
  },
  "scripts": {
    "test": "./node_modules/.bin/ava",
    "transpile": "./node_modules/.bin/buble -i src/ -o dist/ --no modules",
    "modules": "./node_modules/.bin/rollup -i dist/index.js -o dist/index.js -f cjs",
    "build": "npm run transpile ; npm run modules",
    "prepublish": "npm run build"
  },
  "ava": {
    "files": [
      "test/*-test.js"
    ],
    "require": [
      "babel-register"
    ]
  },
  "babel": {
    "presets": [
      "es2015",
      "stage-2"
    ]
  }
}
